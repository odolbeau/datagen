{
    "name": "odolbeau/datagen",
    "description": "A simple lib to deal with data.",
    "keywords": ["fixtures","database","dbal","symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/http-kernel": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "doctrine/doctrine-bundle": "^1.8",
        "symfony/yaml": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Bab\\Datagen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bab\\Datagen\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always"
        ]
    }
}
