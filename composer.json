{
    "name": "odolbeau/simple-dbal-fixtures",
    "description": "A simple lib to deal with DBAL fixtures.",
    "keywords": ["fixtures","doctrine","dbal","symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Bab\\SimpleFixtures\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bab\\SimpleFixtures\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always"
        ]
    }
}
